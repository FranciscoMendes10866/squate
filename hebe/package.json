  "main": "server.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^3.0.1",
    "@types/ioredis": "^4.22.3",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.2",
    "fastify-compress": "^3.4.2",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.3.1",
    "fastify-jwt": "^2.4.0",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.5.0",
    "fastify-redis": "^4.3.0",
    "ioredis": "^4.26.0",
    "pino-pretty": "^4.7.1",
    "prom-client": "^13.1.0"
  },
  "scripts": {
    "dev": "ts-node-dev --cls -r tsconfig-paths/register --respawn --transpile-only --no-notify src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored --ignore '**/*.spec.ts'",
    "start": "node build/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix src --ext .ts",
    "test": "cross-env NODE_ENV=TESTING jest --setupFiles dotenv/config --testTimeout=10000"
  }
}
